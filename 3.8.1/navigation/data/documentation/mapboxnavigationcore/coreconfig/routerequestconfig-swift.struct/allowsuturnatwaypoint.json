{"hierarchy":{"paths":[["doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore","doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"sections":[],"metadata":{"externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"kind":"text","text":": "},{"preciseIdentifier":"s:Sb","kind":"typeIdentifier","text":"Bool"}],"symbolKind":"property","modules":[{"name":"MapboxNavigationCore"}],"role":"symbol","title":"allowsUTurnAtWaypoint","roleHeading":"Instance Property"},"kind":"symbol","abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"type":"text","text":" "},{"text":"waypoint.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"]}],"identifier":{"url":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","interfaceLanguage":"swift"},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["swift"],"platforms":["iOS"],"tokens":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"}]}]},{"content":[{"anchor":"discussion","text":"Discussion","type":"heading","level":2},{"inlineContent":[{"text":"If the value of this property is ","type":"text"},{"code":"true","type":"codeVoice"},{"type":"text","text":", a returned route may require an immediate U-turn at an"},{"type":"text","text":" "},{"text":"intermediate","type":"text"},{"type":"text","text":" "},{"text":"waypoint. At an intermediate waypoint, if the value of this property is ","type":"text"},{"code":"false","type":"codeVoice"},{"text":", each returned route may","type":"text"},{"type":"text","text":" "},{"type":"text","text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two"},{"type":"text","text":" "},{"type":"text","text":"waypoints are specified."}],"type":"paragraph"},{"inlineContent":[{"text":"Set this property to ","type":"text"},{"type":"codeVoice","code":"true"},{"text":" if you expect the user to traverse each leg of the trip separately. For","type":"text"},{"type":"text","text":" "},{"type":"text","text":"example, it"},{"type":"text","text":" "},{"type":"text","text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and"},{"type":"text","text":" "},{"text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to ","type":"text"},{"code":"false","type":"codeVoice"},{"type":"text","text":" if"},{"type":"text","text":" "},{"type":"text","text":"you"},{"type":"text","text":" "},{"type":"text","text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only"},{"text":" ","type":"text"},{"type":"text","text":"needs to"},{"text":" ","type":"text"},{"text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"U-turn"},{"type":"text","text":" "},{"type":"text","text":"at that location."},{"type":"text","text":" "},{"type":"text","text":"The default value of this property is `false."}],"type":"paragraph"}],"kind":"content"}],"schemaVersion":{"major":0,"patch":0,"minor":3},"references":{"doc://com.mapbox.navigation/documentation/MapboxNavigationCore/CoreConfig":{"url":"\/documentation\/mapboxnavigationcore\/coreconfig","type":"topic","navigatorTitle":[{"kind":"identifier","text":"CoreConfig"}],"abstract":[{"text":"Mutable Core SDK configuration.","type":"text"}],"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig","kind":"symbol","title":"CoreConfig","role":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"text":"CoreConfig","kind":"identifier"}]},"doc://com.mapbox.navigation/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/allowsUTurnAtWaypoint":{"kind":"symbol","title":"allowsUTurnAtWaypoint","fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"}],"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","role":"symbol","type":"topic","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint","abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"text":" ","type":"text"},{"text":"waypoint.","type":"text"}]},"doc://com.mapbox.navigation/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct":{"abstract":[{"type":"text","text":"Mutable Routing configuration."}],"url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct","role":"symbol","kind":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"RouteRequestConfig"}],"type":"topic","title":"CoreConfig.RouteRequestConfig","navigatorTitle":[{"text":"RouteRequestConfig","kind":"identifier"}]},"doc://com.mapbox.navigation/documentation/MapboxNavigationCore":{"url":"\/documentation\/mapboxnavigationcore","type":"topic","abstract":[{"text":"The Navigation Core framework provides the basic components for the custom navigation experience.","type":"text"}],"title":"MapboxNavigationCore","role":"collection","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore","kind":"symbol"}}}