tioncore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"},"doc://com.mapbox.navigation/documentation/Overview":{"kind":"article","abstract":[],"type":"topic","title":"MapboxNavigation Documentation","role":"collection","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/Overview","url":"\/documentation\/overview"},"doc://com.mapbox.navigation/documentation/MapboxNavigationCore":{"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore","kind":"symbol","type":"topic","url":"\/documentation\/mapboxnavigationcore","abstract":[{"type":"text","text":"The Navigation Core framework provides the basic components for the custom navigation experience."}],"title":"MapboxNavigationCore","role":"collection"}}}