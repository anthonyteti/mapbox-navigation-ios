n/documentation/MapboxDirections/RouteStep/codes":{"role":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteStep\/codes","fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"codes","kind":"identifier"},{"text":": [","kind":"text"},{"preciseIdentifier":"s:SS","text":"String","kind":"typeIdentifier"},{"kind":"text","text":"]?"}],"abstract":[{"type":"text","text":"Any route reference codes assigned to the road or path leading from this step’s maneuver to the next step’s"},{"type":"text","text":" "},{"type":"text","text":"maneuver."}],"kind":"symbol","url":"\/documentation\/mapboxdirections\/routestep\/codes","title":"codes","type":"topic"},"doc://com.mapbox.navigation/documentation/MapboxDirections/RouteStep":{"abstract":[{"text":"A ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteStep"},{"text":" object represents a single distinct maneuver along a route and the approach to the next maneuver.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"The route step object corresponds to a single instruction the user must follow to complete a portion of the route."},{"text":" ","type":"text"},{"text":"For example, a step might require the user to turn then follow a road.","type":"text"}],"url":"\/documentation\/mapboxdirections\/routestep","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteStep","type":"topic","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"text":"RouteStep","kind":"identifier"}],"title":"RouteStep","navigatorTitle":[{"kind":"identifier","text":"RouteStep"}],"kind":"symbol"}}}