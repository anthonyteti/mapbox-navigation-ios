pboxDirections/RouteOptions":{"navigatorTitle":[{"text":"RouteOptions","kind":"identifier"}],"type":"topic","abstract":[{"type":"text","text":"A "},{"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteOptions","isActive":true,"type":"reference"},{"type":"text","text":" object is a structure that specifies the criteria for results returned by the Mapbox Directions"},{"text":" ","type":"text"},{"type":"text","text":"API."}],"kind":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteOptions","url":"\/documentation\/mapboxdirections\/routeoptions","title":"RouteOptions","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"RouteOptions","kind":"identifier"}],"role":"symbol"},"doc://com.mapbox.navigation/documentation/MapboxDirections/ProfileIdentifier/automobile":{"url":"\/documentation\/mapboxdirections\/profileidentifier\/automobile","abstract":[{"text":"The returned directions are appropriate for driving or riding a car, truck, or motorcycle.","type":"text"}],"kind":"symbol","fragments":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"keyword","text":"let"},{"text":" ","kind":"text"},{"text":"automobile","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:16MapboxDirections17ProfileIdentifierV","text":"ProfileIdentifier","kind":"typeIdentifier"}],"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/ProfileIdentifier\/automobile","title":"automobile","type":"topic","role":"symbol"}}}