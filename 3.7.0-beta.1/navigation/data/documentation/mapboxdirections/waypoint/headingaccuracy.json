documentation\/MapboxDirections\/RouteOptions","isActive":true},{"type":"text","text":" object using waypoint objects and also receive waypoint objects in the completion handler of the"},{"text":" ","type":"text"},{"code":"Directions.calculate(_:completionHandler:)","type":"codeVoice"},{"text":" method.","type":"text"}],"kind":"symbol","url":"\/documentation\/mapboxdirections\/waypoint","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"Waypoint","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"Waypoint"}]},"doc://com.mapbox.navigation/documentation/MapboxDirections/Waypoint/headingAccuracy":{"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"headingAccuracy","kind":"identifier"},{"text":": ","kind":"text"},{"text":"LocationDirection","kind":"typeIdentifier","preciseIdentifier":"s:4Turf17LocationDirectiona"},{"text":"?","kind":"text"}],"type":"topic","url":"\/documentation\/mapboxdirections\/waypoint\/headingaccuracy","kind":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/Waypoint\/headingAccuracy","role":"symbol","abstract":[{"type":"text","text":"The maximum amount, in degrees, by which a route’s approach to a waypoint may differ from "},{"type":"reference","isActive":true,"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/Waypoint\/heading"},{"type":"text","text":" in either"},{"text":" ","type":"text"},{"type":"text","text":"direction in order to be considered viable."}],"title":"headingAccuracy"},"doc://com.mapbox.navigation/documentation/MapboxDirections/Waypoint/heading":{"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"heading"},{"text":": ","kind":"text"},{"text":"LocationDirection","kind":"typeIdentifier","preciseIdentifier":"s:4Turf17LocationDirectiona"},{"text":"?","kind":"text"}],"type":"topic","url":"\/documentation\/mapboxdirections\/waypoint\/heading","