":"keyword"},{"text":" ","kind":"text"},{"text":"CoreConfig","kind":"identifier"}],"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig","title":"CoreConfig","url":"\/documentation\/mapboxnavigationcore\/coreconfig","abstract":[{"type":"text","text":"Mutable Core SDK configuration."}],"kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"CoreConfig"}],"type":"topic","role":"symbol"}}}