{"hierarchy":{"paths":[["doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections","doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteStep"]]},"sections":[],"identifier":{"url":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteStep\/destinationCodes","interfaceLanguage":"swift"},"metadata":{"modules":[{"name":"MapboxDirections"}],"role":"symbol","fragments":[{"kind":"keyword","text":"let"},{"text":" ","kind":"text"},{"kind":"identifier","text":"destinationCodes"},{"kind":"text","text":": ["},{"preciseIdentifier":"s:SS","text":"String","kind":"typeIdentifier"},{"text":"]?","kind":"text"}],"symbolKind":"property","roleHeading":"Instance Property","title":"destinationCodes","externalID":"s:16MapboxDirections9RouteStepV16destinationCodesSaySSGSgvp"},"variants":[{"paths":["\/documentation\/mapboxdirections\/routestep\/destinationcodes"],"traits":[{"interfaceLanguage":"swift"}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","abstract":[{"type":"text","text":"Any route reference codes that appear on guide signage for the road leading from this step’s maneuver to the"},{"text":" ","type":"text"},{"type":"text","text":"next step’s maneuver."}],"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"destinationCodes"},{"text":": [","kind":"text"},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"text":"]?","kind":"text"}],"languages":["swift"]}],"kind":"declarations"},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"type":"paragraph","inlineContent":[{"text":"This property is typically available in steps leading to or from a freeway or expressway. This property contains","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"route reference codes associated with a road later in the route. If a highway ramp is itself part of a 