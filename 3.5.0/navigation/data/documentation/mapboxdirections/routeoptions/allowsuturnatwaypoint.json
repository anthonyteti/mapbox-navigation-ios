eAvoidingTraffic":{"fragments":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"keyword","text":"let"},{"text":" ","kind":"text"},{"kind":"identifier","text":"automobileAvoidingTraffic"},{"kind":"text","text":": "},{"preciseIdentifier":"s:16MapboxDirections17ProfileIdentifierV","text":"ProfileIdentifier","kind":"typeIdentifier"}],"title":"automobileAvoidingTraffic","type":"topic","kind":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/ProfileIdentifier\/automobileAvoidingTraffic","url":"\/documentation\/mapboxdirections\/profileidentifier\/automobileavoidingtraffic","abstract":[{"text":"The returned directions are appropriate for driving or riding a car, truck, or motorcycle.","type":"text"}],"role":"symbol"},"doc://com.mapbox.navigation/documentation/MapboxDirections/ProfileIdentifier/automobile":{"fragments":[{"kind":"keyword","text":"static"},{"kind":"text","text":" "},{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"text":"automobile","kind":"identifier"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:16MapboxDirections17ProfileIdentifierV","text":"ProfileIdentifier"}],"title":"automobile","type":"topic","kind":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/ProfileIdentifier\/automobile","url":"\/documentation\/mapboxdirections\/profileidentifier\/automobile","abstract":[{"text":"The returned directions are appropriate for driving or riding a car, truck, or motorcycle.","type":"text"}],"role":"symbol"}}}