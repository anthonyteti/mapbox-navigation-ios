entifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteStep","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"RouteStep"}],"kind":"symbol","role":"symbol","type":"topic","abstract":[{"type":"text","text":"A "},{"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteStep","isActive":true,"type":"reference"},{"type":"text","text":" object represents a single distinct maneuver along a route and the approach to the next maneuver."},{"text":" ","type":"text"},{"text":"The route step object corresponds to a single instruction the user must follow to complete a portion of the route.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"For example, a step might require the user to turn then follow a road."}],"url":"\/documentation\/mapboxdirections\/routestep","navigatorTitle":[{"text":"RouteStep","kind":"identifier"}]},"doc://com.mapbox.navigation/documentation/MapboxDirections/RouteStep/destinationCodes":{"type":"topic","abstract":[{"type":"text","text":"Any route reference codes that appear on guide signage for the road leading from this step’s maneuver to the"},{"text":" ","type":"text"},{"text":"next step’s maneuver.","type":"text"}],"kind":"symbol","role":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteStep\/destinationCodes","title":"destinationCodes","fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"destinationCodes"},{"text":": [","kind":"text"},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"kind":"text","text":"]?"}],"url":"\/documentation\/mapboxdirections\/routestep\/destinationcodes"}}}